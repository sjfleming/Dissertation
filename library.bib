journal = {Biophysical journal},
keywords = {Cell Membrane Permeability,Diffusion,Electromagnetic Fields,Kinetics,Models, Biological,Molecular Weight,Polymers,Polymers: chemistry,Polymers: metabolism,Porins,Porins: metabolism,Reproducibility of Results},
month = {oct},
