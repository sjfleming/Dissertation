journal = {Physical Review E},
number = {January},
pages = {1--8},
title = {{Measuring the number and spacing of molecular motors propelling a gliding microtubule}},
url = {http://pre.aps.org/abstract/PRE/v83/i1/e011918},
volume = {011918},
year = {2011}
}
