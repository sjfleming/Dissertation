keywords = {helicase,replication,single-molecule studies,translocation,unwinding mechanisms},
number = {November},
pages = {1--11},
title = {{Single-molecule perspectives on helicase mechanisms and functions}},
url = {http://www.tandfonline.com/doi/full/10.3109/10409238.2015.1102195},
volume = {9238},
year = {2015}
}
