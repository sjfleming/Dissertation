year = {2008}
}
@article{Krishnakumar2013,
abstract = {Nanopores were fabricated with an integrated microscale Pd electrode coated with either a hydrogen-bonding or hydrophobic monolayer. Bare pores, or those coated with octanethiol, translocated single-stranded DNA with times of a few microseconds per base. Pores functionalized with 4(5)-(2-mercaptoethyl)-1H-imidazole-2-carboxamide slowed average translocation times, calculated as the duration of the event divided by the number of bases translocated, to about 100 $\mu$s per base at biases in the range of 50 to 80 mV.},
