journal = {Biophys. J.},
number = {1 Pt 1},
pages = {193--205},
pmid = {11751308},
publisher = {Elsevier},
title = {{Dynamics of nucleotides in VDAC channels: structure-specific noise generation.}},
url = {http://dx.doi.org/10.1016/S0006-3495(02)75386-1},
volume = {82},
year = {2002}
}
@article{Lieberman2012,
abstract = {Complexes formed between the bacteriophage phi29 DNA polymerase (DNAP) and DNA fluctuate between the pre-translocation and post-translocation states on the millisecond time scale. These fluctuations can be directly observed with single-nucleotide precision in real-time ionic current traces when individual complexes are captured atop the $\alpha$-hemolysin nanopore in an applied electric field. We recently quantified the equilibrium across the translocation step as a function of applied force (voltage), active-site proximal DNA sequences, and the binding of complementary dNTP. To gain insight into the mechanism of this step in the DNAP catalytic cycle, in this study, we have examined the stochastic dynamics of the translocation step. The survival probability of complexes in each of the two states decayed at a single exponential rate, indicating that the observed fluctuations are between two discrete states. We used a robust mathematical formulation based on the autocorrelation function to extract the forward and reverse rates of the transitions between the pre-translocation state and the post-translocation state from ionic current traces of captured phi29 DNAP-DNA binary complexes. We evaluated each transition rate as a function of applied voltage to examine the energy landscape of the phi29 DNAP translocation step. The analysis reveals that active-site proximal DNA sequences influence the depth of the pre-translocation and post-translocation state energy wells and affect the location of the transition state along the direction of the translocation.},
author = {Lieberman, Kate R and Dahl, Joseph M and Mai, Ai H and Akeson, Mark and Wang, Hongyun},
doi = {10.1021/ja3090302},
file = {:Users/Stephen/Library/Application Support/Mendeley Desktop/Downloaded/Lieberman et al. - 2012 - Dynamics of the translocation step measured in individual DNA polymerase complexes.pdf:pdf},
issn = {1520-5126},
journal = {Journal of the American Chemical Society},
keywords = {Alpha-hemolysin,Biocatalysis,DNA,DNA-Directed DNA Polymerase,DNA-Directed DNA Polymerase: chemistry,DNA-Directed DNA Polymerase: metabolism,DNA: chemistry,DNA: metabolism,Phi29,Thermodynamics},
mendeley-tags = {Alpha-hemolysin,DNA,Phi29},
month = {nov},
number = {45},
pages = {18816--23},
pmid = {23101437},
title = {{Dynamics of the translocation step measured in individual DNA polymerase complexes}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3535181{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {134},
year = {2012}
}
