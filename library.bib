issn = {0956-5663},
journal = {Biosensors {\&} bioelectronics},
keywords = {Animals,Cell Line,Cell Survival,Electrodes,Electroporation,Electroporation: instrumentation,Electroporation: methods,Humans,Hydrogen-Ion Concentration,Metals,Metals: chemistry,Transfection},
month = {apr},
number = {9},
pages = {1353--60},
pmid = {18242073},
title = {{A novel electroporation method using a capillary and wire-type electrode.}},
