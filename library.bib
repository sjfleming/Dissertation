issn = {1748-3395},
journal = {Nature nanotechnology},
keywords = {Animals,Cell Line, Tumor,Cell Membrane Permeability,Cell Membrane Permeability: physiology,Cell Survival,Cell Survival: physiology,Computer Simulation,Electroporation,Electroporation: instrumentation,Electroporation: methods,Equipment Design,Humans,Jurkat Cells,Mice,Microfluidics,Microfluidics: methods,Models, Biological,Nanotechnology,Nanotechnology: methods,Transfection,Transfection: methods},
month = {nov},
number = {11},
pages = {747--54},
pmid = {22002097},
title = {{Nanochannel electroporation delivers precise amounts of biomolecules into living cells.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22002097},
volume = {6},
year = {2011}
}
@article{Liao2012,
abstract = {We report a new approach, molecular dam, to enhance mass transport for protein enrichment in nanofluidic channels by nanoscale electrodeless dielectrophoresis under physiological buffer conditions. Dielectric nanoconstrictions down to 30 nm embedded in nanofluidic devices serve as field-focusing lenses capable of magnifying the applied field to 10(5)-fold when combined with a micro- to nanofluidic step interface. With this strong field and the associated field gradient at the nanoconstrictions, proteins are enriched by the molecular damming effect faster than the trapping effect, to {\textgreater}10(5)-fold in 20 s, orders of magnitude faster than most reported methods. Our study opens further possibilities of using nanoscale molecular dams in miniaturized sensing platforms for rapid and sensitive protein analysis and biomarker discovery, with potential applications in precipitation studies and protein crystallization and possible extensions to small-molecules enrichment or screening.},
author = {Liao, Kuo Tang and Chou, Chia Fu},
doi = {10.1021/ja3016523},
file = {:Users/Stephen/Documents/Stephen/Research/Articles/Liao protein dielectrophoresis - JACS 2012.pdf:pdf},
