@article{Kowalczyk2012,
abstract = {The charge of a DNA molecule is a crucial parameter in many DNA detection and manipulation schemes such as gel electrophoresis and lab-on-a-chip applications. Here, we study the partial reduction of the DNA charge due to counterion binding by means of nanopore translocation experiments and all-atom molecular dynamics (MD) simulations. Surprisingly, we find that the translocation time of a DNA molecule through a solid-state nanopore strongly increases as the counterions decrease in size from K(+) to Na(+) to Li(+), both for double-stranded DNA (dsDNA) and single-stranded DNA (ssDNA). MD simulations elucidate the microscopic origin of this effect: Li(+) and Na(+) bind DNA stronger than K(+). These fundamental insights into the counterion binding to DNA also provide a practical method for achieving at least 10-fold enhanced resolution in nanopore applications.},
author = {Kowalczyk, Stefan W and Wells, David B and Aksimentiev, Aleksei and Dekker, Cees},
doi = {10.1021/nl204273h},
file = {:Users/Stephen/Library/Application Support/Mendeley Desktop/Downloaded/Kowalczyk et al. - 2012 - Slowing down DNA translocation through a nanopore in lithium chloride.pdf:pdf},
issn = {1530-6992},
journal = {Nano letters},
keywords = {DNA,DNA: chemistry,Lithium Chloride,Lithium Chloride: chemistry,Models, Molecular,Molecular Dynamics Simulation,Nanopores,Nanotechnology,Particle Size,Surface Properties},
month = {feb},
number = {2},
pages = {1038--44},
pmid = {22229707},
title = {{Slowing down DNA translocation through a nanopore in lithium chloride.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3349906{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {12},
year = {2012}
}
@article{Serebrov2004,
abstract = {The NS3 helicase is essential for cytoplasmic RNA replication by the hepatitis C virus, and it is a representative member of helicase superfamily 2 (SF2). NS3 is an important model system for understanding unwinding activities of DExH/D proteins, and it has been the subject of extensive structural and mutational analyses. Despite intense interest in NS3, the molecular and kinetic mechanisms for RNA unwinding by this helicase have remained obscure. We have developed a combinatorial, time-resolved approach for monitoring the microscopic behaviour of a helicase at each nucleotide of a duplex substrate. By applying this analysis to NS3, we have independently established the 'physical' and 'kinetic' step size for unwinding of RNA (18 base pairs, in each case), which we relate to the stoichiometry of the functional, translocating species. Having obtained microscopic unwinding rate constants at each position along the duplex, we demonstrate that NS3 unwinds RNA through a highly coordinated cycle of fast ripping and local pausing that occurs with regular spacing along the duplex substrate, much like the stepping behaviour of cytoskeletal motor proteins.},
author = {Serebrov, Victor and Pyle, Anna Marie},
doi = {10.1038/nature02704},
file = {:Users/Stephen/Documents/Stephen/Research/Articles/Serebrov NS3 helicase unwinding RNA - Nature 2004.pdf:pdf},
isbn = {1476-4687 (Electronic)},
issn = {0028-0836},
journal = {Nature},
number = {6998},
pages = {476--480},
pmid = {15269774},
title = {{Periodic cycles of RNA unwinding and pausing by hepatitis C virus NS3 helicase.}},
volume = {430},
year = {2004}
}
