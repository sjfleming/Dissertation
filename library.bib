journal = {Journal of Biological Chemistry},
number = {10},
pages = {6482--6494},
pmid = {25589786},
title = {{A parallel quadruplex DNA is bound tightly but unfolded slowly by Pif1 helicase}},
volume = {290},
year = {2015}
}
